    <% if (user.role=='admin' || user.role=='instructor') { %>
      <div class="col-sm-6 col-sm-offset-3">
      <h3>Change Application Status</h3>
    <form name="applicationstatus" method="post">
        <div class="form-group">
          <label>Application Status</label>
          <select class="form-control" name="applicationstatus">
          <option value="" >Select a status</option>
          <option value="complete" >Complete</option>
          <option value="incomplete">Incomplete</option>
          <option value="further review">Further Review</option>
          <option value="applicant rescinded" >Applicant Rescinded</option>
          <option value="applicant postponed">Applicant Postponed</option>
          <option value="site declined">Site Declined</option>
      </select>
        </div>
        <button type="submit" class="btn btn-success btn-lg">Change Status</button>
    </form>
    </div>
      <div class="col-sm-6 col-sm-offset-3">
      <h3>Notes</h3>
        <table class="table table-bordered">
    <thead>
      <tr>
        <th>Note</th>
        <th>Entered By</th>
        <th>Date</th>
</tr>
    </thead>

    <tbody>
          <% application.notes.forEach(function(note) { %>
          <tr>
              <td><%= note.note %></td>
              <td><%= note.user %></td>
              <td><%= note.date %></td>
          </tr>
          <% }); %>
      </tbody>
      </table>
      <hr>
      </div>
      <div class="col-sm-6 col-sm-offset-3">
    <form name="applicationstatus" action="/application/itec/notes/<%= application._id %>" method="post">
