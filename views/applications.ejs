    <!doctype html>
<html>
<head>
    <title>Add Site</title>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css"> <!-- load bootstrap css -->
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css"> <!-- load fontawesome -->
    <style>
        body        { padding-top:80px; }
    </style>
</head>
<body>
<% include partials/nav.ejs %>

    <div class="container">
  <h2>Site Details</h2>
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>Email</th>
        <th>User ID</th>
        <th>Application Status</th>
        <th>Proposed Internship Semester</th>
        <th>Proposed Internship Year</th>
        <th>Classification</th>
        <th>Year</th>
        <th>GPA</th>
        <th>ITEC GPA</th>
        <th>ITEC 2150</th>
        <th>ITEC 3100</th>
        <th>ITEC 3200</th>
        <th>ITEC 3900</th>
        <th>Emergency Contact Name</th>
        <th>Emergency Contact Address</th>
        <th>Emergency Contact Email</th>
        <th>Emergency Contact City</th>
        <th>Emergency Contact Zipcode</th>
        <th>Emergency Contact Phone</th>
        <th>Mentor Name</th>
        <th>Mentor Phone</th>
        <th>Mentor Email</th>
        <th>Mentor Office</th>
        <th>Interests and Strengths</th>
        <th>Type of Internship</th>
        <th>Focus</th>
        <th>Have Proposed Internship</th>
        <th>Proposed Org Name</th>
        <th>Proposed org address</th>
        <th>Proposed org city</th>
        <th>Proposed org state</th>
        <th>Proposed org phone</th>
        <th>Proposed org manager</th>
        <th>Is employee at site</th>
        <th>Is it a paid position</th>
        <th>Number of hours worked</th>
        <th>Is it a seperate project</th>
        <th>Can they do it seperately</th>
        <th>User Signature</th>
        <th>User Printed Name</th>
      </tr>
    </thead>
    <tbody>
        <% applicationList.forEach(function(application) { %>
          <tr>
            <td><%= application.email %></td>
            <td><%= application.id %></td>
    	<td><%= application.applicationstatus %></td>
    	<td><%= application.proposedinternsemester %></td>
    	<td><%= application.proposedinternyear %></td>
    	<td><%= application.major %></td>
    	<td><%= application.classification %></td>
    	<td><%= application.gpa %></td>
    	<td><%= application.itecgpa %></td>
    	<td><%= application.itec2150 %></td>
    	<td><%= application.itec3100 %></td>
    	<td><%= application.itec3200 %></td>
    	<td><%= application.itec3900 %></td>
    	<td><%= application.emergencycontactname %></td>
    	<td><%= application.emergencycontactaddress %></td>
    	<td><%= application.emergencycontactemail %></td>
    	<td><%= application. emergencycontactcity %></td>
    	<td><%= application.emergencycontactzipcode %></td>
    	<td><%= application.emergencycontactphone %></td>
    	<td><%= application.mentorname %></td>
    	<td><%= application.mentorcellphone %></td>
    	<td><%= application.mentoremail %></td>
    	<td><%= application.mentoroffice %></td>
    	<td><%= application.interestsandstrengths %></td>
    	<td><%= application.typeofinternship %></td>
    	<td><%= application.focusonsoftdev %></td>
    	<td><%= application.haveproposedintern %></td>
    	<td><%= application.proposedorgname%></td>
    	<td><%= application.itecfocus %></td>
    	<td><%= application.proposedorgaddress %></td>
    	<td><%= application.proposedorgcity %></td>
    	<td><%= application.proposedorgstate %></td>
    	<td><%= application.proposedorgzipcode %></td>
    	<td><%= application.proposedorgphone %></td>
    	<td><%= application.proposedorgmanager %></td>
    	<td><%= application.isemployedatsite %></td>
    	<td><%= application.employedcurrentposition %></td>
    	<td><%= application.ispaidposition %></td>
    	<td><%= application.numberofhoursworked %></td>
    	<td><%= application.isseparateproject %></td>
    	<td><%= application.isseparatehours %></td>
    	<td><%= application.signature %></td>
    	<td><%= application.printedname %></td>
          </tr>
          <% }); %>
    </tbody>
  </table>
</div>
    </body>
    </html>
